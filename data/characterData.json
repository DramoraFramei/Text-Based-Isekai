{
    "races": {
        "human": {
            "racialAbilities": {
                "adaptability": {
                    "name": "Adaptability",
                    "description": "Allows them to adapt quickly to most situations.",
                    "effect": {
                        "adapt": {
                            "description": "situations are less stressfull.",
                            "cooldown": "10 seconds",
                            "success chance": "50%"
                        }
                    }
                }
            }
        },
        "elf": {
            "racialAbilities": {
                "elvenMagic": {
                    "name": "Elven Magic",
                    "description": "Elves are naturally attuned to nature and nature magic so they are more powerful when it comes to magic and dealing with plants, animals, and nature-based situations.",
                    "effect": {
                        "magic afinity": {
                            "description": "Elves are more attuned to magic and master nature-based spells much faster than other races.",
                            "cooldown": "10 seconds",
                            "success chance": "50%"
                        }
                    }
                },
                "natureAffinity": {
                    "name": "Nature Affinity",
                    "description": "Elves are more attuned to nature and are far more skilled at dealing with nature-based situations.",
                    "effect": {
                        "nature affinity": {
                            "description": "Nature-based situations are less stressful.",
                            "cooldown": "10 seconds",
                            "success chance": "50%"
                        }
                    }
                }
            }
        },
        "beastkin": {
            "racialAbilities": {
                "primalSenses": {
                    "name": "Primal Senses",
                    "description": "Possesses the heightened senses of a wild animal, making them difficult to ambush.",
                    "effect": {
                        "keenSenses": {
                            "description": "Passively increases chance to detect hidden enemies, traps, and tracks.",
                            "cooldown": "Passive",
                            "success chance": "75%"
                        }
                    }
                }
            }
        },
        "elemental": {
            "racialAbilities": {
                "elementalAffinity": {
                    "name": "Elemental Affinity",
                    "description": "As a being of raw elemental energy, you are resistant to your own element but vulnerable to its opposite.",
                    "effect": {
                        "elementalResistance": {
                            "description": "Grants resistance to a specific element (e.g., fire, ice) but also a weakness to its opposing element.",
                            "cooldown": "Passive",
                            "success chance": "100%"
                        }
                    }
                }
            }
        },
        "undead": {
            "racialAbilities": {
                "willOfTheGrave": {
                    "name": "Will of the Grave",
                    "description": "Your undead nature makes you immune to poison and disease, and resistant to being charmed or put to sleep.",
                    "effect": {
                        "undeadNature": {
                            "description": "Immune to poison, disease, and sleep effects. Vulnerable to holy and fire damage.",
                            "cooldown": "Passive",
                            "success chance": "100%"
                        }
                    }
                }
            }
        },
        "dwarf": {
            "racialAbilities": {
                "stonecunning": {
                    "name": "Stonecunning",
                    "description": "Dwarves have a natural affinity for stone and metal, granting them superior crafting and mining skills.",
                    "effect": {
                        "masterCraftsman": {
                            "description": "Increased success chance and quality when crafting or mining. Can spot unusual stonework.",
                            "cooldown": "Passive",
                            "success chance": "100%"
                        }
                    }
                },
                "dwarvenResilience": {
                    "name": "Dwarven Resilience",
                    "description": "Their hardy nature gives them a natural resistance to poisons and magic.",
                    "effect": {
                        "hardy": {
                            "description": "High chance to resist poison and magical effects.",
                            "cooldown": "Passive",
                            "success chance": "60%"
                        }
                    }
                }
            }
        },
        "orc": {
            "racialAbilities": {
                "battleRage": {
                    "name": "Battle Rage",
                    "description": "In the heat of battle, an orc can enter a rage, ignoring pain and fighting with savage intensity.",
                    "effect": {
                        "berserk": {
                            "description": "Temporarily increases attack and strength, but lowers defense. Can be activated once per combat.",
                            "cooldown": "1 combat",
                            "success chance": "100%"
                        }
                    }
                }
            }
        },
